generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "mysql"
}

model Turmas {
  id Int @id @default(autoincrement())
  nome String
  quantidade Int 
  alunos Alunos[]
}

model Alunos {
  id Int @id @default(autoincrement())
  nome String
  matricula String
  turmaId Int
  turmas Turmas @relation(fields: [turmaId], references: [id]) 
}